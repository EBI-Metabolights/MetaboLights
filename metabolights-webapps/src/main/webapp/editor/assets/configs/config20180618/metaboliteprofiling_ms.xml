<isatab-config-file xmlns="http://www.ebi.ac.uk/bii/isatab_configuration#">
    <isatab-configuration table-name="metaboliteprofiling_ms" isatab-assay-type="ms_spec_assay"
                          isatab-conversion-target="meda">
        <measurement term-label="metabolite profiling" term-accession="http://purl.obolibrary.org/obo/OBI_0000366" source-abbreviation="OBI"/>
        <technology term-label="mass spectrometry" term-accession="http://purl.obolibrary.org/obo/OBI_0000470" source-abbreviation="OBI"/>
        <field header="Sample Name" data-type="String" is-file-field="false" is-multiple-value="true" is-required="true"
               is-hidden="false" is-forced-ontology="false">
            <description>
                <![CDATA[Samples represent major outputs resulting from a protocol application other than the special case outputs of Instrument or a sample extract.]]></description>
            <default-value><![CDATA[]]></default-value>
            <generated-value-template>[INSTITUTION].Group-[GROUP_NO].Subject-[SUBJECT_NO].[SAMPLE_EXTRACT]
            </generated-value-template>
        </field>
        <protocol-field protocol-type="Extraction"/>
        <field header="Parameter Value[Post Extraction]" data-type="String" is-file-field="false"
               is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
            <description>
                <![CDATA[Extract concentration, Samples enrichment and resuspension processes e.g Dried under nitrogen, resuspended in H2O or pyridine, SPE column]]></description>
            <default-value><![CDATA[]]></default-value>
        </field>
        <field header="Parameter Value[Derivatization]" data-type="List" is-file-field="false" is-multiple-value="false"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[Derivatization methods e.g. OMS, TMS]]></description>
            <default-value><![CDATA[]]></default-value>
            <list-values>sylilation,acetylation,alkylation,other,none</list-values>
        </field>
        <field header="Extract Name" data-type="String" is-file-field="false" is-multiple-value="false"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[User-defined names for each portion of extracted material.]]></description>
            <default-value><![CDATA[]]></default-value>
            <generated-value-template>
                [INSTITUTION].Group-[GROUP_NO].Subject-[SUBJECT_NO].[SAMPLE_EXTRACT].Extract-[EXTRACT_COUNT]
            </generated-value-template>
        </field>
        <protocol-field protocol-type="Chromatography"/>
        <field header="Parameter Value[Chromatography Instrument]" data-type="Ontology term" is-file-field="false"
               is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[Chromatography Instrument, manufacturer and model]]></description>
            <default-value></default-value>
        </field>
        <field header="Parameter Value[Column model]" data-type="String" is-file-field="false" is-multiple-value="false"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[Model or make of the of Column used, e.g C18]]></description>
            <default-value><![CDATA[]]></default-value>
        </field>
        <field header="Parameter Value[Column type]" data-type="String" is-file-field="false" is-multiple-value="false"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[type of Chromatography used e.g reverse phase]]></description>
            <default-value><![CDATA[]]></default-value>
        </field>
        <field header="Labeled Extract Name" data-type="String" is-file-field="false" is-multiple-value="false"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[Identifier for the labeled extract.]]></description>
            <default-value><![CDATA[]]></default-value>
            <generated-value-template>
                [INSTITUTION].Group-[GROUP_NO].Subject-[SUBJECT_NO].[SAMPLE_EXTRACT].Extract-[EXTRACT_COUNT].LE-[LABEL_COUNT]
            </generated-value-template>
        </field>
        <field header="Label" data-type="Ontology term" is-file-field="false" is-multiple-value="false"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description>
                <![CDATA[Indicates a chemical or biological marker, such as a radioactive isotope or a fluorescent dye which is bound to a material in order to make it detectable by some assay technology (e.g. P33, biotin, GFP).]]></description>
            <default-value></default-value>
            <recommended-ontologies>
                <ontology id="" abbreviation="CHEBI" name="Chemical Entities of Biological Interest"
                          version="Jun 2010"/>
            </recommended-ontologies>
        </field>
        <protocol-field protocol-type="Mass spectrometry"/>
        <field header="Parameter Value[Scan polarity]" data-type="List" is-file-field="false" is-multiple-value="false"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description>
                <![CDATA[An acquisition mode to which specifies weather polarity is negative, positive or alternating.]]></description>
            <default-value><![CDATA[]]></default-value>
            <list-values>positive,negative,alternating,n/a</list-values>
        </field>
        <field header="Parameter Value[Scan m/z range]" data-type="String" is-file-field="false"
               is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
            <description>
                <![CDATA[The limit of m/z over which a mass spectrometer can detect ions. e.g 200-1000]]></description>
            <default-value><![CDATA[]]></default-value>
        </field>
        <field header="Parameter Value[Instrument]" data-type="Ontology term" is-file-field="false"
               is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
            <description>
                <![CDATA[the name of the mass spectrometry instrument, manufacturer, model # operational software name operational software version]]></description>
            <default-value></default-value>
            <recommended-ontologies>
                <ontology id="" abbreviation="MS" name="PSI Mass Spectrometry Ontology" version="Jun 2010"/>
                <ontology id="" abbreviation="SBO" name="Systems Biology Ontology" version="Jun 2010"/>
                <ontology id="" abbreviation="PSI" name="Mass Spectroscopy CV (PSI-MS)" version="Jun 2010"/>
            </recommended-ontologies>
        </field>
        <field header="Parameter Value[Ion source]" data-type="Ontology term" is-file-field="false"
               is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
            <description>
                <![CDATA[the ion source use PSI-MS cv Ionization mode (EI, APCI, ESI#), vacuum pressure skimmer/focusing lens voltages (e.g. capillary voltage etc.) gas flows (e.g. nebulization gas, cone gas etc.) source temperature]]></description>
            <default-value></default-value>
            <recommended-ontologies>
                <ontology id="" abbreviation="MS" name="PSI Mass Spectrometry Ontology" version="Jun 2010"/>
                <ontology id="" abbreviation="SBO" name="Systems Biology Ontology" version="Jun 2010"/>
                <ontology id="" abbreviation="PSI" name="Mass Spectroscopy CV (PSI-MS)" version="Jun 2010"/>
            </recommended-ontologies>
        </field>
        <field header="Parameter Value[Mass analyzer]" data-type="Ontology term" is-file-field="false"
               is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[the analyzer(s) used by the mass spectrometer, use PSI-MS cv]]></description>
            <default-value></default-value>
            <recommended-ontologies>
                <ontology id="" abbreviation="MS" name="PSI Mass Spectrometry Ontology" version="Jun 2010"/>
                <ontology id="" abbreviation="SBO" name="Systems Biology Ontology" version="Jun 2010"/>
                <ontology id="" abbreviation="PSI" name="Mass Spectroscopy CV (PSI-MS)" version="Jun 2010"/>
            </recommended-ontologies>
        </field>
        <field header="MS Assay Name" data-type="String" is-file-field="false" is-multiple-value="false"
               is-required="true" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[User-defined name for a MS assay.]]></description>
            <default-value><![CDATA[]]></default-value>
            <generated-value-template>
                [INSTITUTION].Group-[GROUP_NO].Subject-[SUBJECT_NO].[SAMPLE_EXTRACT].Extract-[EXTRACT_COUNT].LE-[LABEL_COUNT].MSASSAY-[HYB_COUNT]
            </generated-value-template>
        </field>
        <field header="Raw Spectral Data File" data-type="String" is-file-field="true" is-multiple-value="true"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[Name (or URI) of the raw spectral data file generated by an assay.]]></description>
            <default-value><![CDATA[]]></default-value>
        </field>
        <protocol-field protocol-type="Data transformation"/>
        <field header="Normalization Name" data-type="String" is-file-field="false" is-multiple-value="false"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[User-defined name for each normalization applied]]></description>
            <default-value><![CDATA[]]></default-value>
        </field>
        <field header="Derived Spectral Data File" data-type="String" is-file-field="true" is-multiple-value="true"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description>
                <![CDATA[Name (or URI) of the derived data file generated from spectral data by an assay.]]></description>
            <default-value><![CDATA[]]></default-value>
        </field>
        <protocol-field protocol-type="Metabolite identification"/>
        <field header="Data Transformation Name" data-type="String" is-file-field="false" is-multiple-value="true"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description><![CDATA[User-defined name for each data transformation applied]]></description>
            <default-value><![CDATA[]]></default-value>
        </field>
        <field header="Metabolite Assignment File" data-type="String" is-file-field="true" is-multiple-value="false"
               is-required="false" is-hidden="false" is-forced-ontology="false">
            <description>
                <![CDATA[Name (or URI) of the metabolite assignment file gathered for this assay.]]></description>
            <default-value><![CDATA[]]></default-value>
        </field>
        <structured-field name="factors"/>
    </isatab-configuration>
</isatab-config-file>
