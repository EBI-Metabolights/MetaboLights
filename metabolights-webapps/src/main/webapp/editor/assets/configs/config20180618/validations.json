{
	"study": {
		"identifier": {
			"data-type": "string",
			"sort-order": "1",
			"is-required": "true",
			"is-read-only": "true",
			"is-hidden": "false",
			"technique": "mass spectrometry|NMR spectroscopy",
			"description": "Initially a user defined identifier for the study. This study id will be replaced with a MetaboLights ID (MTBLS) when uploaded to MetaboLights",
			"placeholder": "MTBLS350",
			"rules": [{
				"condition": "min",
				"value": "8",
				"error": "Study identifier should be at least 8 characters long",
				"type": "string"
			},
			{
				"condition": "match",
				"value": "MTBLS*",
				"error": "Study identifier must start with MTBLS, followed by a number. Example: MTBLS350",
				"type": "string"
			}
		]
		},
		"title": {
			"data-type": "string",
			"sort-order": "2",
			"is-required": "true",
			"is-read-only": "false",
			"is-hidden": "false",
			"technique": "mass spectrometry|NMR spectroscopy",
			"description": "A title for the Study. Please use the same title as the first publication",
			"placeholder": "Example: Metabolic study of Diabetes in Homo sapiens...",
			"rules": [{
				"condition": "min",
				"value": "8",
				"error": "Study title should be at least 8 characters long. Please use the same title as the first publication.",
				"type": "string"
			}]
		},
		"description": {
			"data-type": "string",
			"sort-order": "3",
			"is-required": "true",
			"is-read-only": "false",
			"is-hidden": "false",
			"technique": "mass spectrometry|NMR spectroscopy",
			"description": "A brief description of the study aims. Please using the abstract of the publication",
			"placeholder": "Please provide a description of your experiment, why not use the abstract of your paper",
			"rules": [{
				"condition": "min",
				"value": "60",
				"error": "Study description should be at least 60 characters long. Please using the abstract of the publication",
				"type": "string"
			}]
		},
