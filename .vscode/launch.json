    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "java",
            "name": "Debug webapp (Attach)",
            "request": "attach",
            "projectName": "metabolights-webapp",
            "hostName": "localhost",
            "port": 8000
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "StrangeCharactersDetector",
            "request": "launch",
            "mainClass": "uk.ac.ebi.metabolights.repository.utils.validation.StrangeCharactersDetector",
            "projectName": "metabolights-dao"
        }
    ]
}